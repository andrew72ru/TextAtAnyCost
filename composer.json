{
  "name": "creative/text-at-any-cost",
  "description": "Package for parse text from old .doc and .rtf files",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Zhdanovskih",
      "email": "azh@crtweb.ru",
      "role": "Refactoring and OOP-style"
    },
    {
      "name": "Алексей Рембиш a.k.a Ramon",
      "email": "alex@rembish.ru",
      "role": "Core developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "doctrine/collections": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "TextAtAnyCost\\": "src"
    },
    "files": [
      "functions/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "config": {
    "preferred-install": {"*": "dist"},
    "sort-packages": true
  }
}
